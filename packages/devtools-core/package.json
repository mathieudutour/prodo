    "@babel/preset-typescript": "^7.6.0",
    "@prodo/babel-plugin": "^0.0.3",
    "@types/node": "^12.7.2",
    "@types/react": "^16.8.25",
    "@types/react-dom": "^16.8.5",
    "parcel-bundler": "^1.12.3",
    "typescript": "^3.6.3"
  }
}
