{
  "name": "@prodo/devtools-core",
  "version": "0.0.5",
  "main": "transpiled/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf .cache build dist lib tsconfig.tsbuildinfo",
    "start": "parcel index.html",
    "build:code": "tsc --build",
    "build:babel": "babel src --out-dir transpiled --extensions .ts,.tsx",
    "build:babel:watch": "yarn build:babel --watch",
    "build:monaco": "./build-monaco.sh",
    "css": "cp src/*.css lib/ && cp src/*.css transpiled/",
    "build": "set -ex; yarn build:monaco; yarn build:code; yarn build:babel"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
