    "@prodo/core": "^0.0.5",
    "@prodo/logger": "^0.0.5",
    "color": "^3.1.2",
    "fast-stable-stringify": "^1.0.0",
    "lodash": "^4.17.15",
    "monaco": "^1.201704190613.0",
    "monaco-editor": "^0.17.0",
    "monaco-editor-textmate": "^2.1.1",
    "monaco-textmate": "^3.0.0",
    "onigasm": "^2.2.2",
    "path-browserify": "^1.0.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.9.0",
    "react-editable-json-tree": "^2.2.1",
    "react-scroll-to-bottom": "^1.3.2",
    "styled-components": "^4.3.2"
  },
