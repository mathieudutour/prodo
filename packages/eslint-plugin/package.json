{
  "name": "@prodo/eslint-plugin",
  "version": "0.0.9",
  "description": "prodo plugin for eslint",
  "files": [
    "src",
    "lib"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf lib tsconfig.tsbuildinfo",
    "test": "jest",
    "lint": "set -ex; tsc --build; tslint --project ."
  },
  "author": "Prodo",
  "main": "lib/index.js",
  "devDependencies": {
    "@types/eslint": "^6.1.1",
    "@typescript-eslint/parser": "^2.1.0",
    "@typescript-eslint/typescript-estree": "^2.1.0",
    "eslint": "^6.3.0",
    "eslint-scope": "^5.0.0",
    "eslint-visitor-keys": "^1.1.0",
    "ts-jest": "^24.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  }
}
