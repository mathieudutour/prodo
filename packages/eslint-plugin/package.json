  "author": "Prodo",
  "main": "lib/index.js",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/experimental-utils": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "@typescript-eslint/typescript-estree": "^2.1.0",
    "eslint": "^6.3.0",
    "eslint-scope": "^5.0.0",
    "eslint-visitor-keys": "^1.1.0",
    "ts-jest": "^24.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
